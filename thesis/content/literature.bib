	pages = {000025--000030},
	file = {Full Text PDF:/Users/andreasbelavic/Zotero/storage/HERYVTLR/Hegedüs et al. - 2018 - Hybrid Trajectory Planning for Autonomous Vehicles.pdf:application/pdf;IEEE Xplore Abstract Record:/Users/andreasbelavic/Zotero/storage/C9PZJGHT/8928220.html:text/html},
