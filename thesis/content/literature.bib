	keywords = {Trajectory, Acceleration, Planning, Cost function, Noise measurement, Optimal control},
	pages = {4569--4574},
	file = {Full Text PDF:/Users/andreasbelavic/Zotero/storage/HAK7LCRG/Kalakrishnan et al. - 2011 - STOMP Stochastic trajectory optimization for moti.pdf:application/pdf;IEEE Xplore Abstract Record:/Users/andreasbelavic/Zotero/storage/WFJ2FBCR/5980280.html:text/html},
